{
    "version": 2,
    "name": "hill-calories-ai",
    "builds": [{
        "src": "**/*",
        "use": "@vercel/static"
    }],
    "routes": [{
            "src": "/webhook/Meal",
            "dest": "https://n8n-rx89.onrender.com/webhook/Meal"
        },
        {
            "src": "/api/(.*)",
            "dest": "https://n8n-rx89.onrender.com/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "headers": [{
            "source": "/(.*)",
            "headers": [{
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "camera=self, microphone=(), geolocation=(), payment=()"
                }
            ]
        },
        {
            "source": "/(.*\\.(css|js))$",
            "headers": [{
                "key": "Cache-Control",
                "value": "public, max-age=31536000, immutable"
            }]
        }
    ]
}